{
  "name": "myigniter",
  "authors": [
    "Asrul Hanafi <hanafi.asrul@gmail.com>"
  ],
  "description": "myigniter assets",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "bootstrap": "~3.3.5",
    "alertify-js": "~1.5.0",
    "font-awesome": "~4.4.0",
    "AdminLTE": "admin-lte#~2.3.2",
    "bootstrap-show-password": "~1.0.2",
    "list.js": "~1.1.1",
    "jquery": "~2.1.4",
    "slimScroll": "jquery.slimscroll#~1.3.6",
    "iCheck": "icheck#~1.0.2",
    "datatables": "~1.10.10",
    "fancybox": "~2.1.5",
    "jquery-nestable": "*",
    "chosen": "~1.4.2"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "./dist/js/bootstrap.min.js",
        "./dist/css/bootstrap.min.css",
        "./dist/fonts/*.*"
      ]
    },
    "alertify-js": {
      "main": [
        "./build/css/alertify.min.css",
        "./build/css/themes/*.*",
        "./build/alertify.min.js"
      ]
    },
    "font-awesome": {
      "main": [
        "./css/font-awesome.min.css",
        "./fonts/*.*"
      ]
    },
    "AdminLTE": {
      "main": [
        "./dist/css/AdminLTE.min.css",
        "./dist/css/skins/*.*",
        "./dist/js/app.min.js",
        "./dist/img/*.*"
      ]
    },
    "slimScroll": {
      "main": "./jquery.slimscroll.min.js"
    },
    "iCheck": {
      "main": [
        "./icheck.min.js",
        "./skins/**/*.*"
      ]
    },
    "bootstrap-show-password": {
      "main": "./bootstrap-show-password.min.js"
    },
    "list.js": {
      "main": "./dist/list.min.js"
    },
    "jquery": {
      "main": "./dist/jquery.min.js"
    },
    "datatables": {
      "main": [
        "./media/css/*.*",
        "./media/images/*.*",
        "./media/js/*.*",
        "./media/extensions/**/**/*.*"
      ]
    },
    "fancybox": {
      "main": "./source/**/*.*"
    },
    "jquery-nestable": {
      "main": [
        "./jquery.nestable.css",
        "./jquery.nestable.js"
      ]
    },
    "chosen": {
      "main": [
        "./*.png",
        "./*.js",
        "./*.css"
      ]
    }
  }
}
